      "use": "@now/node"
    },
    {
      "src": "middlewares/*.js",
      "use": "@now/node-server"
    },
    {
      "src": "auth/*.js",
      "use": "@now/node-server"
    }
  ],
  "env": {
    "GOOGLE_CLIENT_ID":"974341650966-lkjjc6me1stsh6jps5dd9lenh765n6ke.apps.googleusercontent.com",
    "GOOGLE_CLIENT_SECRET":"lwVBZaJ-USRlNbtfibTZ4wsA"
  },
  "routes": [
    {
      "src": "/",
      "dest": "app.js"
    },
    {
      "src": "/auth/google",
      "dest": "app.js"
    },
    {
      "src": "/auth/google/callback",
      "dest": "app.js"      
    }
  ]
}